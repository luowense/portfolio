<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome on Laurent Segura Portfolio{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Zhi+Mang+Xing&display=swap" rel="stylesheet">
            <link rel="stylesheet" type="text/css" href="/assets/css/creative.min.css">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fullcalendar/core@4.1.0/main.min.css">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fullcalendar/daygrid@4.1.0/main.min.css">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fullcalendar/timegrid@4.1.0/main.min.css">
            <script src="https://kit.fontawesome.com/338b1a41c2.js" crossorigin="anonymous"></script>
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body id="page-top">
        {% block header %}
            {% include 'layout/navbar.html.twig' %}
        {% endblock %}
        {% block body %}
        {% endblock %}
        {% block javascripts %}
        {% endblock %}
    </body>
